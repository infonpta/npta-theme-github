                                        {% assign options = block.settings.options | split: ', ' %}
                                        {% for option in options %}
                                            <option value="{{ option | handleize }}">{{ option }}</option>
                                        {% endfor %}
                                    </select>
                            {% endcase %}
                        </div>

                        {% assign block_count = block_count | plus: 1 %}

                        {% if block_count == 2 or forloop.last %}
                            </div>
                            {% assign block_count = 0 %}
                        {% endif %}
                    {% endif %}
                {% endfor %}

